class Test2 {
    private int a, b;
    public Test2() {
      a = 1; // Значения по умолчанию
      b = 5;
    }
    public Test2(int a_, int b_) {
      a = a_;
      b = b_;
    }
    public int get_a() {
        return a;
    }
    public int get_b() {
        return b;
    }
    public void set_a(int a_) {
      a = a_;
    }
    public void set_b(int b_) {
      b = b_;
    }
}
